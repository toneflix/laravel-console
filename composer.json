{
    "name": "toneflix-code/laravel-console",
    "description": "A Laravel package to help with frequent artisan tasks and more.",
    "keywords": [
        "toneflix-code",
        "laravel-console"
    ],
    "homepage": "https://github.com/toneflix-code/laravel-console",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Toneflix Code",
            "email": "code@toneflix.com.ng",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "illuminate/support": "^8.0|^9.0",
        "illuminate/filesystem": "^8.0|^9.0",
        "masbug/flysystem-google-drive-ext": "^2.2",
        "spatie/laravel-slack-alerts": "^1.1",
        "madnest/madzipper": "^1.3"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "ToneflixCode\\LaravelConsole\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ToneflixCode\\LaravelConsole\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "ToneflixCode\\LaravelConsole\\LaravelConsoleServiceProvider"
            ],
            "aliases": {
                "LaravelConsole": "ToneflixCode\\LaravelConsole\\LaravelConsoleFacade"
            }
        }
    }
}